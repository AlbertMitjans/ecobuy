{
  "manifest_version": 2,
 
  "name": "EcoBuy",
  "version": "0.1.0",
  "description": "EcoBuy Plug-in Extension",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["math.js", "jquery-3.4.1.js", "jquery.lettering.js", "content.js", "circle_type.js"],
    "matches": ["https://www.ebay.com/*"]
  }],

  "icons": { "16": "images/16.png",
  			"32": "images/32.png",
           "48": "images/48.png",
          "128": "images/128.png" }
}
